		System.out.println("enter the string :");
		String str=s.nextLine();
		String s1=new StringBuffer(str).reverse().toString();
		//str.reverse(s1);
		
		if(str.equalsIgnoreCase(s1)) {
		//if(str==s1){
			System.out.println("palindrom");
		}
		
		else {
			System.out.println("not palindrom");
		}
	}
}
